!			'E6CB29', #yellow
!			'4D79FF', #blue
!			'AA3DBF', #magenta
!			'3CB6F3', #cyan
!			'DBDDE4', #white
!		],
!
!! ... or in .profile/.bash_profile/.bashrc, etc.:
!
